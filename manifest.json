{
  "manifest_version": 3,
  "name": "Auto Meet Joiner",
  "version": "1.0",
  "description": "Automatically join Google Meet links at scheduled times.",
  "permissions": ["storage", "notifications", "tabs", "alarms"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  }
}